<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

	<definition name="/layout" template="/WEB-INF/views/layout/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/views/layout/header.jsp"/>
		<put-attribute name="body" value="/WEB-INF/views/layout/body.jsp"/>
		<put-attribute name="footer" value="/WEB-INF/views/layout/footer.jsp"/>
	</definition>
	
	<definition name="/*" extends="/layout">
    	  <put-attribute name="body" value="/WEB-INF/views/resume/{1}.jsp" />
	</definition>
	
	<definition name="/Ajax/*" template="/WEB-INF/views/layout/layoutAjax.jsp">
		<put-attribute name="body1" value="/WEB-INF/views/resume/{1}.jsp" />
	</definition>

</tiles-definitions>